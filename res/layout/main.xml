<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent">
	<TextView android:id="@+id/gameText" android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:text="Welcome to the pokedex!" />
	<Button android:id="@+id/noButton" android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:text="no" />
	<Button android:id="@+id/yesButton" android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:text="yes" />
	<EditText android:id="@+id/editText" android:layout_width="225px"
		android:layout_height="44px" android:text="EditText" android:textSize="12sp">
	</EditText>
</LinearLayout>
